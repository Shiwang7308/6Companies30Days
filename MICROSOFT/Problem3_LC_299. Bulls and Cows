class Solution {
public:
    string getHint(string secret, string guess) {
    unordered_map<char,int>s;
    unordered_map<char,int>g;

        for(int i=0;i<secret.size();i++)
        {
              s[secret[i]]++;
        }
         for(int i=0;i<guess.size();i++)
        {
              g[guess[i]]++;
        }
        string ans="";
        int x=0;
        int y=0;
        
        for(int i=0;i<guess.size();i++)
        {
            if(guess[i]==secret[i])
            {
                x++;
                g[guess[i]]--;
                s[guess[i]]--;
                if(g[guess[i]]==0)
                g.erase(guess[i]);
                if(s[secret[i]]==0)
                s.erase(secret[i]);
            }
           
        }
        for(int i=0;i<guess.size();i++)
        {      
               if(secret[i]==guess[i])
               continue;

               if(s.find(guess[i])!=s.end())
                {
                    y++;
                    
                s[guess[i]]--;
                if(s[guess[i]]==0)
                s.erase(guess[i]);
                }
        }
        ans+=to_string(x);
        ans+='A';
        ans+=to_string(y);
        ans+='B';
        return ans;
    }
};